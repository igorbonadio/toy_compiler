X1 = Alphabet(a, c, g, t)
X2 = Alphabet(CODING, NONCODING)
Y = Alphabet(Exon, Intron)

gene_pedictor = HMM([X1, X2], Y):
  initial_probabilities = Prob(
    Exon   = 0.5
    Intron = 0.5
  )
  transitions = Prob(
    Exon   | Exon   = 0.3
    Intron | Exon   = 0.7
    Exon   | Intron = 0.1
    Intron | Intron = 0.9
  )
  emissions = Prob(
    a; CODING | Exon   = 0.25
    c; CODING | Exon   = 0.25
    t; CODING | Exon   = 0.25
    g; CODING | Exon   = 0.25
    a; CODING | Intron = 0.50
    c; CODING | Intron = 0.25
    t; CODING | Intron = 0.05
    g; CODING | Intron = 0.20
  )